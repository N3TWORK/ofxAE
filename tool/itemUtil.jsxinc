function getItemFolder(item) {	var ret = '';	while(item.parentFolder && item.parentFolder !== app.project.rootFolder) {		ret = item.parentFolder.name+'/'+ret;		item = item.parentFolder;	}	return ret;};function copyItem(item, dir) {	if(!item.file) return;	var file = new File(item.file);	var dir = new Folder(dir+'/'+getItemFolder(item));	if(!dir.exists){		dir.create();	}	var newFileName = dir.toString()+'/'+file.toString().replace(/.*\//,"");	if(file.toString() !== newFileName) {        	$.writeln(newFileName);		file.copy(newFileName);	}};function copySequenceItem(item, dir) {    $.writeln("copySequenceItem.name: " + item.name);	if(item.name.toString().match(/(.*)\[([0-9]+)-([0-9]+)](.+)/)) {		var before = RegExp.$1;		var start = RegExp.$2;		var end = RegExp.$3;		var after = RegExp.$4;		var len = start.length;		var dir = new Folder(dir+'/'+getItemFolder(item));		if(!dir.exists){			dir.create();		}		before = item.file.toString().replace(/[^\/]*$/, "") + before;		for(var i = Number(start); i <= Number(end); ++i) {			var numstr = (Array(len+1).join('0')+i).slice(-len);			var file = new File(before+numstr+after);			var newFileName = dir.toString()+'/'+file.toString().replace(/.*\//,"");			if(file.toString() !== newFileName) {				file.copy(newFileName);			}		}	}}